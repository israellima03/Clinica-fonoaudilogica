abla Especialidad
CREATE TABLE Especialidad (
    Id_especialidad INTEGER PRIMARY KEY AUTOINCREMENT,
    Nombre_especialidad TEXT NOT NULL
);

-- Tabla Personal (antes Profesional)
CREATE TABLE Personal (
    Id_personal INTEGER PRIMARY KEY AUTOINCREMENT,
    Nombre TEXT NOT NULL,
    Apellido_paterno TEXT NOT NULL,
    Apellido_materno TEXT,
    Carnet TEXT UNIQUE,
    Edad INTEGER,
    Id_especialidad INTEGER,
    Telefono TEXT,
    FOREIGN KEY (Id_especialidad) REFERENCES Especialidad(Id_especialidad)
);

-- Tabla Persona (antes Paciente)
CREATE TABLE Persona (
    Id_persona INTEGER PRIMARY KEY AUTOINCREMENT,
    Nombre TEXT NOT NULL,
    Apellido_paterno TEXT NOT NULL,
    Apellido_materno TEXT,
    Carnet TEXT UNIQUE,
    Fecha_nacimiento DATE,
    Edad INTEGER,
    Genero TEXT,
    Telefono TEXT,
    Direccion TEXT
);

-- Tabla Tratamiento
CREATE TABLE Tratamiento (
    Id_tratamiento INTEGER PRIMARY KEY AUTOINCREMENT,
    Nombre_Tratamiento TEXT NOT NULL,
    Descripcion TEXT,
    Duracion TEXT
);

-- Tabla Persona_tratamiento (antes Paciente_Tratamiento)
CREATE TABLE Persona_Tratamiento (
    Id_pertratamiento INTEGER PRIMARY KEY AUTOINCREMENT,
    Id_persona INTEGER,
    Id_tratamiento INTEGER,
    Fecha_inicio DATE,
    Fecha_fin DATE,
    Estado TEXT,
    FOREIGN KEY (Id_persona) REFERENCES Persona(Id_persona),
    FOREIGN KEY (Id_tratamiento) REFERENCES Tratamiento(Id_tratamiento)
);

-- Tabla Cita (fecha y hora separadas)
CREATE TABLE Cita (
    Id_cita INTEGER PRIMARY KEY AUTOINCREMENT,
    Id_persona INTEGER,
    Id_personal INTEGER,
    Fecha DATE,
    Hora TIME,
    Estado TEXT,
    FOREIGN KEY (Id_persona) REFERENCES Persona(Id_persona),
    FOREIGN KEY (Id_personal) REFERENCES Personal(Id_personal)
);

-- Tabla Historial Clinico (fecha y hora separadas)
CREATE TABLE Historial_Clinico (
    Id_historial INTEGER PRIMARY KEY AUTOINCREMENT,
    Id_persona INTEGER,
    Id_personal INTEGER,
    Fecha DATE,
    Hora TIME,
    Diagnostico TEXT,
    Observaciones TEXT,
    FOREIGN KEY (Id_persona) REFERENCES Persona(Id_persona),
    FOREIGN KEY (Id_personal) REFERENCES Personal(Id_personal)
);</sql><current_tab id="0"/></tab_sql></sqlb_project>
